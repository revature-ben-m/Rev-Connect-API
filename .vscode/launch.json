    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        },
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "RevConnectApiApplication",
            "request": "launch",
            "mainClass": "com.rev_connect_api.RevConnectApiApplication",
            "projectName": "rev-connect-api"
        }
    ]
}